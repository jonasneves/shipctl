{
  "name": "shipctl",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:local": "tsc && vite build --base=/ --outDir dist",
    "fetch-models": "node scripts/fetch-models.mjs",
    "sync": "node scripts/sync-apps.js",
    "build:extension": "npm run sync && npm run fetch-models && ./node_modules/.bin/tsc && ./node_modules/.bin/vite build --config vite.config.extension.ts",
    "preview": "npm run build:local && vite preview --outDir dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "lucide-react": "^0.561.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tslib": "^2.8.1",
    "xstate": "^5.25.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.32",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "vite": "^5.4.11"
  }
}